      "source": [
        "class Solution:\n",
        "    def smallerNumbersThanCurrent(self, nums: List[int]) -> List[int]:\n",
        "        ans=[]\n",
        "        list1=sorted(nums)\n",
        "        for i in nums:\n",
        "            ans.append(list1.index(i))\n",
        "        return ans\n",
        "\n"
      ]
    }
  ]
}