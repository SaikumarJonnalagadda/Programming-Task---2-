      "source": [
        "class Solution:\n",
        "    def subtractProductAndSum(self, n: int) -> int:\n",
        "        add=0\n",
        "        mul=1\n",
        "        while n>0:\n",
        "            a=n%10\n",
        "            n=n//10\n",
        "            add+=a\n",
        "            mul=mul*a\n",
        "        return (mul-add)"
      ]
    }
  ]
}